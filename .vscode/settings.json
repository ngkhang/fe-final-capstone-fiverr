{
  // Config Issue: Unknown at rule @tailwindcss(unknownAtRules)
  "files.associations": {
    "*.css": "tailwindcss",
  },
  // Setup Format and Lint
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.fixAll.ts": "always"
  },
  "eslint.run": "onType",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.eol": "\r\n",
  "prettier.endOfLine": "crlf",
}